char *str;
int count;
str = SysMalloc(40);
str = GetTagChar("@CurrentUser");
printf("%s",str);
count = strlen(str);
if(count >0)
{
printf("%s  |",SysFree(str));
return 1;
}
else
{
printf("%s  |",SysFree(str));
return 0;
}
